{
  "canisters": {
    "backend": {
      "candid": "src/backend/backend.did",
      "package": "backend",
      "type": "rust",
      "optimize": "cycles",
      "declarations": {
        "output": "src/frontend/src/declarations/backend"
      }
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "source": [
        "src/frontend/build"
      ],
      "type": "assets",
      "workspace": "src/frontend",
      "optimize": "size"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": ""
    }
  },
  "output_env_file": ".env",
  "version": 1
}
